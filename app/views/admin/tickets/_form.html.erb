<%= render 'common/errors', resource: @ticket %>
<div class="row">
  <%= form_for [:admin, @ticket] do |f| %>
    <%= f.hidden_field :train_id %>
    <%= f.hidden_field :first_station_id %>
    <%= f.hidden_field :last_station_id %>
    <div class="col-md-4">
      <%= f.label :user_name%>
      <%= f.text_field :user_name, class: "form-control" %>
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-4">
      <%= f.label :passport%>
      <%= f.text_field :passport, class: "form-control" %>
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-4">
      <%= f.submit t('common.update'), class: "btn btn-success" %>
    </div>
  <% end %>
</div>
<br />
