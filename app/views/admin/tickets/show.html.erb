<div class="page-header">
  <h1><%= t('.title') %> <small></small></h1>
</div>
<p>
  <table class="table table-striped">
    <thead>
      <th><%= t('admin.tickets.attributes.fio')%></th>
      <th><%= t('admin.tickets.attributes.passport')%></th>
      <th><%= t('admin.trains.show.title') %></th>
      <th><%= t('admin.tickets.attributes.route')%></th>
      <th><%= t('admin.tickets.attributes.first_station')%></th>
      <th><%= t('admin.tickets.attributes.last_station')%></th>
    </thead>

    <tbody>
      <tr align="center">
        <td><%= @ticket.user_name %></td>
        <td><%= @ticket.passport %></td>
        <td><%= @ticket.train.number %></td>
        <td><%= @ticket.train.route.name %></td>
        <td><%= @ticket.train.route.railway_stations.first.title %></td>
        <td><%= @ticket.train.route.railway_stations.last.title %></td>
      </tr>
    </tbody>
  </table>
</p>

