<%= render 'common/errors', resource: @user %>
<div class="row">
  <%= form_for [:admin, user] do |f| %>
      <div class="col-md-4">
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
      </div>
</div>
<br />
<div class="row">
      <div class="col-md-4">
      <%= f.label :last_name %>
      <%= f.text_field :last_name, class: "form-control" %>
      </div>
</div>
<br />
<div class="row">
      <div class="col-md-4">
      <%= f.label :email %>
      <%= f.text_field :email, class: "form-control" %>
      </div>
</div>
<br />
<div class="row">
      <div class="col-md-4">
      <%= f.submit t('common.update'), class: 'btn btn-success' %>
      </div>
  <% end %>
</div>
<br />
