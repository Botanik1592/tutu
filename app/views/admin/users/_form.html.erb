<%= form_for [:admin, user] do |f| %>
  <%= render 'common/errors', resource: @user %>
  <div>
    <%= f.label :name, "Имя:" %>
    <%= f.text_field :name %>
    <br /><br />
    <%= f.label :last_name, "Фамилия:" %>
    <%= f.text_field :last_name %>
    <br /><br />
    <%= f.label :email, "Email:" %>
    <%= f.text_field :email %>
    <br /><br />
    <%= f.submit "Изменить"%>
  </div>
<% end %>
