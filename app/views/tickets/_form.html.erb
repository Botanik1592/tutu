<%= form_for @ticket do |f| %>
    <%= render 'common/errors', resource: @ticket %>
    <%= f.hidden_field :train_id %>
    <%= f.hidden_field :first_station_id %>
    <%= f.hidden_field :last_station_id %>
    <%= f.label :user_name, "ФИО:" %>
    <%= f.text_field :user_name %>
    <br /><br />
    <%= f.label :passport, "Паспорт (серия/номер):" %>
    <%= f.text_field :passport %>
    <br /><br />
    <%= f.submit 'Оформить'%>
  <% end %>

